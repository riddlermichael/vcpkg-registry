{
  "name": "rubidium",
  "version": "0.2.0",
  "description": "Uniscan's company-wide C++ library designed to extend the C++ standard library",
  "homepage": "https://github.com/riddlermichael/rubidium",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "full-source-location-info",
    "time-v2"
  ],
  "features": {
    "duration-has-nan": {
      "description": "time::Duration has NaN value"
    },
    "full-source-location-info": {
      "description": "Print full source location info"
    },
    "time-v2": {
      "description": "Use time::v2"
    }
  }
}
